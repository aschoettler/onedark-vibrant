  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A737D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage", "storage.type.function", "storage.type.class", "keyword.control.rust", "storage.type.rust"],
      "settings": {
        "foreground": "#C69AF7"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "meta.function-call.generic",
        "entity.name.function.member"
      ],
      "settings": {
        "foreground": "#6DB3F2"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#76E0EA"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#8CCF7E"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#E0C787"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF9E64"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#76E0EA"
      }
    },
    {
      "scope": ["entity.name.type", "storage.type", "entity.name.class"],
      "settings": {
        "foreground": "#E0C787"
      }
    },
    {
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#76E0EA"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F7768E"
      }
    },
    {
      "scope": ["variable.parameter", "meta.function.parameters variable"],
      "settings": {
        "foreground": "#DCDFE4"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#F7768E"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F7768E"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#DCDFE4"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#F7768E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#6A737D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#E0C787",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#C69AF7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#8CCF7E"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#DCDFE4"
      }
    },
    {
      "scope": ["string.other.link.title", "string.other.link.description"],
      "settings": {
        "foreground": "#6DB3F2"
      }
    },
    {
      "scope": ["markup.underline.link", "markup.link.url"],
      "settings": {
        "foreground": "#C69AF7"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#F7768E"
      }
    }
  ]
}
